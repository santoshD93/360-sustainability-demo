{
    "builds": [
      { "src": "api/**/*.ts", "use": "@vercel/node" },
      { "src": "frontend/package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "api/$1.ts" },
      { "src": "/(.*)", "dest": "frontend/$1" }
    ]
  }
  